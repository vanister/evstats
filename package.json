{
  "name": "evstats-ionic",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --logLevel error",
    "preview": "vite preview",
    "test": "vitest",
    "test.lcov": "vitest run --coverage",
    "test.lcov.all": "vitest run --coverage --coverage.all",
    "test.debug": "vitest --watch=false",
    "test.once": "vitest --watch=false",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "start": "npm run dev -- --port=8100",
    "start.ios": "dotenv -c -- bash -c 'npx vite build && npx cap copy ios && npx cap run ios --target=$IOS_TARGET_DEVICE --live-reload --port=$DEV_PORT --host=$DEV_HOST & npx vite --host=$DEV_HOST --port=$DEV_PORT --mode=development'",
    "start.logserver": "node utilities/devLogServer.js"
  },
  "dependencies": {
    "@capacitor-community/sqlite": "7.0.0",
    "@capacitor/app": "7.0.1",
    "@capacitor/core": "7.2.0",
    "@capacitor/filesystem": "7.1.1",
    "@capacitor/haptics": "7.0.1",
    "@capacitor/ios": "7.2.0",
    "@capacitor/keyboard": "7.0.1",
    "@capacitor/preferences": "7.0.1",
    "@capacitor/share": "^7.0.2",
    "@capacitor/splash-screen": "7.0.1",
    "@capacitor/status-bar": "7.0.1",
    "@ionic/react": "8.5.8",
    "@ionic/react-router": "8.5.8",
    "@reduxjs/toolkit": "2.8.2",
    "@types/react-redux": "^7.1.34",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "chart.js": "4.4.9",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "immer": "^10.1.1",
    "ionicons": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "6.0.0",
    "react-redux": "^9.2.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4"
  },
  "devDependencies": {
    "@capacitor/cli": "7.2.0",
    "@eslint/js": "9.28.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/eslint": "9.6.1",
    "@types/minimist": "^1.2.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-legacy": "6.1.1",
    "@vitejs/plugin-react": "4.5.0",
    "@vitest/coverage-v8": "2.1.9",
    "cors": "^2.8.5",
    "dotenv-cli": "^10.0.0",
    "eslint-plugin-react": "7.37.5",
    "express": "^4.21.2",
    "globals": "^15.6.0",
    "jsdom": "^22.1.0",
    "minimist": "^1.2.8",
    "sass": "1.89.1",
    "terser": "5.40.0",
    "typescript": "5.9.2",
    "typescript-eslint": "^7.14.1",
    "vite": "6.3.5",
    "vitest": "2.1.9"
  }
}
